{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "text": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "text"
    },
    "description": {
      "type": "blocks"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "order": {
      "type": "integer"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "frames",
        "accessories"
      ]
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "categories"
    }
  }
}
